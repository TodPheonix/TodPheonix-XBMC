<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.1channel-proxyfix" name="1Channel Proxyfix" version="1.0.1" provider-name="TodPheonix">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="plugin.video.1channel" version="2.0.2"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">1Channel Proxyfix</summary>
    <description lang="en">This temporary fix will enable immediate use of the PrimewireProxy.co.uk service for 1Channel users blocked due to censorship in the United Kingdom, allowing you to once again use 1Channel freely.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.filmontv" name="FilmOn" version="1.1" provider-name="FilmOn.TV Networks Inc">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.t0mm0.common" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">FilmOn</summary>
    <language>en de fr es it ru</language>
    <description lang="en">FilmOn.com is an internet based television provider owned by FilmOn.TV Networks Inc. FilmOn.TV Networks was founded by Alki David in 2006.FilmOn.com subscription packages include access to over 120 streaming TV channels (mostly in English language) in addition to premium FilmOn.com Movie channels and pay-per-view channels</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.TodPheonix" name="TodPheonix's XBMC Add-on Repository" version="1.0" provider-name="TodPheonix">
	<extension point="xbmc.addon.repository" name="TodPheonix's XBMC Addon Repository">
		<info compressed="false">http://github.com/TodPheonix/TodPheonix-xbmc/raw/master/addons.xml</info>
		<checksum>http://github.com/TodPheonix/TodPheonix-xbmc/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">http://github.com/TodPheonix/TodPheonix-xbmc/raw/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>TodPheonix's XBMC Add-on Repository</summary>
		<description>TodPheonix's XBMC Add-on Repository</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
